{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "ES2022",
        "jsx": "react-jsx",
        "allowImportingTsExtensions": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true
    },
    "typeAcquisition": {
        "include": [
            "jquery"
        ]
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}